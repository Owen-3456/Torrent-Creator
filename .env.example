# Torrent Creator Example Environment Variables
# Copy this file to .env and fill in your own values

# TMDB API Key (do not commit real key)
TMDB_API_KEY=your_tmdb_api_key_here

# TVDB API Key (do not commit real key)
TVDB_API_KEY=your_tvdb_api_key_here

# Output directory for processed torrents
OUTPUT_DIRECTORY=/path/to/output

# Release group name
RELEASE_GROUP=GROUP

# Tracker announce URLs (comma separated)
TRACKERS=https://example.com/announce

# SMB/Network share settings (optional)
REMOTE_PATH_ENABLED=false
REMOTE_PATH=

# Notes for NFO files
NFO_NOTES=Enjoy and seed!

# ASCII art for NFO header (optional)
NFO_ASCII_ART=custom_ascii_or_default
